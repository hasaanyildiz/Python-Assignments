   ],
   "source": [
    "prime_list = []\n",
    "\n",
    "limit_num = int(input(\"Enter a limit number to print prime numbers:\"))\n",
    "\n",
    "for i in range(2,limit_num+1):\n",
    "\n",
    "    for j in range(2, i):\n",
    "\n",
    "        if i % j == 0:\n",
    "\n",
    "            break\n",
    "\n",
    "    else:\n",
    "\n",
    "        prime_list.append(i)\n",
    "\n",
    "\n",
    "\n",
    "print(prime_list)"
   ]
